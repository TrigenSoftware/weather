{
	"exclude": "node_modules/**",
	"presets": [
		["@babel/preset-env", { "modules": false }],
		["@babel/preset-stage-0", {
			"decoratorsLegacy": true,
			"pipelineProposal": "minimal"
		}],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-transform-runtime"
	],
	"overrides": [{
		"include": "scripts/**/*",
		"presets": [
			["@babel/preset-env", {
				"targets": { "node": "current" }
			}],
			["@babel/preset-stage-0", {
				"decoratorsLegacy": true,
				"pipelineProposal": "minimal"
			}]
		]
	}]
}
